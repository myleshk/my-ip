{
	"functions": {
		"predeploy": [
			"npm --prefix \"$RESOURCE_DIR\" run lint"
		],
		"source": "functions"
	},
	"hosting": {
		"public": "frontend/dist",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites": [
			{
				"source": "/myip",
				"function": "us"
			},
			{
				"source": "**",
				"destination": "/index.html"
			}
		]
	}
}